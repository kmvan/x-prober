{
  "name": "x-prober",
  "description": "A PHP prober",
  "scripts": {
    "dev": "cross-env WEBPACK_ENV=development webpack --watch",
    "build": "cross-env WEBPACK_ENV=production webpack",
    "test": "cross-env WEBPACK_ENV=test webpack"
  },
  "browserslist": [
    "last 4 version"
  ],
  "babel": {
    "plugins": [
      "@babel/transform-runtime",
      "@babel/plugin-transform-strict-mode",
      "@babel/plugin-transform-arrow-functions",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/transform-async-to-generator",
        {
          "module": "bluebird",
          "method": "coroutine"
        }
      ],
      "lodash"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "include": [
            "transform-arrow-functions",
            "es6.map"
          ],
          "targets": {
            "browsers": [
              "last 4 version"
            ]
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-transform-strict-mode": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/classnames": "^2.2.7",
    "@types/core-js": "^2.5.0",
    "@types/lodash-es": "^4.17.2",
    "@types/node": "^11.9.4",
    "@types/query-string": "^6.2.0",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "classlist-polyfill": "^1.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "lodash-es": "^4.17.11",
    "lodash-webpack-plugin": "^0.11.5",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "node-libs-browser": "^2.2.0",
    "node-sass": "^4.11.0",
    "number-precision": "^1.3.1",
    "object-assign": "^4.1.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss": "^7.0.14",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "sass-loader": "^7.1.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-common-shake": "^2.1.0",
    "webpack-log": "^2.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "css-loader": "^2.1.0",
    "es6-promise": "^4.2.6",
    "glob": "^7.1.3",
    "isomorphic-fetch": "^2.2.1",
    "jsonp-promise": "^0.1.2",
    "query-string": "^6.2.0",
    "react-classnames": "^0.1.2",
    "style-loader": "^0.23.1"
  }
}
